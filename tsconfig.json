{
  "compilerOptions": {
    "module": "nodeNext",
    "moduleResolution": "NodeNext",
    "baseUrl": "src",
    "resolveJsonModule": true,
    "sourceMap": true,
    "outDir": "dist",
    "esModuleInterop": true,
  },
  "include": [
    "./src/**/*"
  ],
  "exclude": [
    "./node_modules",
    "./dist"
  ],
}